{
  "name": "react_mobile_demo",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://zhipenglin.github.io/react_mobile_demo",
  "dependencies": {
    "classnames": "^2.2.5",
    "history": "^4.6.3",
    "isomorphic-fetch": "^2.2.1",
    "query-string": "^5.0.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-loadable": "^4.0.3",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "react-router-redux": "next",
    "react-scripts": "1.0.10",
    "recompose": "^0.24.0",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0"
  },
  "scripts": {
    "predeploy": "REACT_APP_TARGET=github npm run build",
    "deploy": "gh-pages -d build -r https://github.com/zhipenglin/react_mobile_demo.git",
    "analyze": "source-map-explorer build/static/js/main.*",
    "precommit": "lint-staged",
    "prepush":"npm run predeploy",
    "start": "npm-run-all -p start-js watch-css",
    "build": "npm run build-css&&react-scripts build",
    "start-js": "react-scripts start",
    "watch-css": "gulp",
    "build-css": "gulp sass",
    "test": "react-scripts test --env=jsdom"
  },
  "devDependencies": {
    "gh-pages": "^1.0.0",
    "gulp": "^3.9.1",
    "gulp-px2rem-transform": "^0.1.0",
    "gulp-sass": "^3.1.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.0.2",
    "npm-run-all": "^4.0.2",
    "prettier": "^1.5.3",
    "source-map-explorer": "^1.4.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  }
}
